declare module 'acorn' {
	declare export type AcornNode = {
		type: string,
		start: number,
		end: number,
		name: string,
		value: string,
		callee: AcornNode,
		source: AcornNode,
		expression: AcornNode,
		body: AcornNode[],
		arguments: AcornNode[],
	};

	declare type AcornConfig = {
		ecmaVersion?: number,
		sourceType?: 'script' | 'module',
		plugins?: {
			[key: string]: boolean,
		},
	};

	declare function parse(code: string, config?: AcornConfig): AcornNode;
}
